          "fieldName": {
            "type": "string",
            "nullable": true
          },
          "fieldType": {
            "type": "string",
            "nullable": true
          },
          "options": {
            "type": "string",
            "nullable": true
          },
          "fieldOrder": {
            "type": "integer",
            "format": "int32"
          }
