            "type": "string",
            "nullable": true
          },
          "mobileNumber": {
            "type": "string",
            "nullable": true
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "otpCode": {
            "type": "string",
            "nullable": true
          },
          "isOtpSent": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
